{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "jsx": "react",
    "lib": ["dom", "es5", "es6", "es7", "es2015", "es2015.promise", "es2017"],
    "typeRoots": ["node_modules/@types", "./**/typings"]
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules"]
}
