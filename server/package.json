{
  "name": "server",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "scripts": {
    "build": "yarn build:clean && yarn build:compile",
    "build:clean": "rm -rf ./build/server",
    "build:compile": "tsc",
    "start": "yarn build:compile && node ./build/server/src/index.js",
    "watch": "yarn build && yarn watch:the:actual:thing",
    "watch:the:actual:thing": "node-dev ./build/server/index.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "koa": "^2.5.2",
    "koa-body": "^4.0.4",
    "koa-mount": "^4.0.0",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "pg-promise": "^10.8.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sql-template-strings": "^2.2.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/koa": "^2.0.46",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-router": "^7.0.30",
    "@types/koa-static": "^4.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "node-dev": "^6.2.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
